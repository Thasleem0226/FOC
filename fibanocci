#include<stdio.h>
void fib(unsigned int nl,unsigned int n2, unsigned int num);  
int main()
{
int n;
printf("n enter the limit:");
scanf(%d,&n);
printf ("frequency sequence upto %d terms\n",n);
fib(0,1,n);
}
void fib(unsigned int nl, unsigned int n2, unsigned int num)
{
if (numl=0)
{
printf("%u \n", nl);
fib (n2,n2+nl,num-l);
}
}
